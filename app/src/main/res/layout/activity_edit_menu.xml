<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditMenu">

    <LinearLayout
        android:id="@+id/menuTable"
        android:layout_width="415dp"
        android:layout_height="661dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"></LinearLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/menuRemoveMealButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:text="Remove"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/menuTable"
        app:layout_constraintEnd_toStartOf="@+id/menuAddMealButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.727"
        tools:visibility="visible" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/menuAddMealButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:text="Add"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/menuTable"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.727"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>